<div class="card">
  <%= link_to image_tag(prototype.image, class: :card__img ),prototype_path(prototype.id), method: :get %>
  <div class="card__body">
    <%= link_to "プロトタイプのタイトル", prototype_path(prototype.id), method: :get, class: :card__title%>
    <%= prototype.title %>
    <p class="card__summary">
      <%= "プロトタイプのキャッチコピー" %>
      <%= prototype.catch_copy %>
    </p>
      <% if user_signed_in? %>
    <%= link_to "by プロトタイプの投稿者名", "/users/#{current_user.id}", class: :card__user %>
    <%= prototype.user.name %>
    <% end %>
  </div>
</div>