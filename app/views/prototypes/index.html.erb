<main class="main">
  <div class="inner">
 <% if user_signed_in? %>
   <div class="greeting"> 
        <%= "こんにちは、" %>
        <%= link_to "#{current_user.name}さん", user_path(current_user), class: :greeting__link%>
      <% else %>
        <%= link_to "新規登録！", new_user_registration_path, class: :greeting_link%>
      <% end %>
       </div> 
    <div class="card__wrapper">
      <%= render partial: "prototype",collection: @prototypes %>
    </div>
  </div>
</main>
